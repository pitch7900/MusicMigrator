{% extends "main.twig" %}
{% import "macros/home.twig" as macrohome %}
{% block interactivemenus %}
    {{include('interactivemenus/loadfile.twig')}}
    {{include('interactivemenus/DeezerLogin.twig')}}
{% endblock %}
{% block content %}

    <div id="main_page" class="container-fluid row">
        

            {% if fileuploaded %}
                <div class="col-sm-4">
                    <div class="sidebar-sticky">
                        <ul class="nav flex-column">
                            {% for playlist in playlists%}
                                {{ macrohome.navplaylistitem(playlist.id,playlist.name) }}
                            {% endfor %}
                        </ul>
                    </div>


                </div>
                        <div class="col-sm-8">
                
                    <div id="songlist" >
                    </div>
                        
                </div>
            {% else %}
                
            {% endif %}



        
    </div>

{% endblock %}


{% block footer %}
    <footer class="footer  py-3 bg-light fixed-bottom">
        <div class="container">
            {{ debugdata }}
        </div>
    </footer>
{%endblock%}